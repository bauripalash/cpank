fun test_assert(msg , value1 , value2)
	show "-----";
	if (!asserteq(value1 , value2)) then
		show msg;
		show "Failed";
		show "-----";
		return 0;
	end

	show value1;
	show value2;

	show "Assert Passed!";
	show "-----";
	return 1;
end


test_assert("1 = 2" , 1 , 2);
test_assert("1+2 = 3" , 1+2 , 3);
